package day9;
import org.testng.annotations.Test;

public class Grouping_ {

	@Test(groups = "A")
    public void openBrowser() {
        System.out.println("A is Running ");
    }

    @Test(groups = "B")
    public void login() {
        System.out.println("B is Running ");
    }

    @Test(groups = {"A", "B"})
   public void logout() {
        System.out.println("Both A,B Running");
    }
	
	
	
}
