package day9;

	import org.testng.annotations.AfterSuite;
	import org.testng.annotations.BeforeSuite;
	import org.testng.annotations.Test;

	public class Suite_levell {

			
			@BeforeSuite
		    public void beforeSuite() {
		        System.out.println("Before Suite - Runs once before the entire test suite.");
		    }

		    @AfterSuite
		    public void afterSuite() {
		        System.out.println("After Suite - Runs once after the entire test suite.");
		    }

		    @Test
		    public void testSuiteLevel() {
		        System.out.println("Test method inside SuiteLevelTest");
		    }
			
			
		}

